{# Purpose of this template: Display images within an external context #}
{% for image in items %}
    <h3>{{ image|rkhelpermodule_formattedTitle }}</h3>
    <p><a href="{{ path('rkhelpermodule_image_display', {'id': image.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
